{
    "name": "Tutanota",
    "id": "tutanota",
    "packaging_format": 1,
    "description": {
        "en": "email service with a strong focus on security and privacy that lets you encrypt emails",
        "fr": ""
    },
    "version": "3.95.1~ynh1",
    "url": "https://github.com/tutao/tutanota",
    "upstream": {
        "license": "free",
        "website": "https://github.com/tutao/tutanota",
        "demo": "https://demo.example.com",
        "admindoc": "https://yunohost.org/packaging_apps",
        "userdoc": "https://yunohost.org/apps",
        "code": "https://github.com/tutao/tutanota"
    },
    "license": "free",
    "maintainer": {
        "name": "John doe",
        "email": "john.doe@example.com"
    },
    "requirements": {
        "yunohost": ">= 4.3.0"
    },
    "multi_instance": true,
    "services": [
        "nginx"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/example",
                "default": "/example"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
