{
    "package_format": 1,
    "name": "Tutanota",
    "id": "tutao",
    "packaging_format": 1,
    "description": {
        "en": "End-To-End Encrypted Mail Client",
        "fr": ""
    },
    "url": "https://github.com/tutao/tutanota/",
    "license": "MIT",
    "version": "0.11",
    "maintainer": {
        "name": "frju365",
        "email": "win10@tutanota.com"
    },
    "multi_instance": false,
    "services": [
        "nginx",
        "mysql"
    ],
    "requirements": {
        "yunohost": ">= 2.4.0"
    },
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "ask": {
                    "en": "Choose a domain for Gogs",
                    "fr": "Choisissez un domaine pour Gogs"
                },
                "example": "domain.org"
            },
            {
                "name": "path",
                "type": "path",
                "ask": {
                    "en": "Choose a path for Gogs",
                    "fr": "Choisissez un chemin pour Gogs"
                },
                "example": "/tutao",
                "default": "/tutao"
            },
            {
                "name": "admin",
                "type": "user",
                "ask": {
                    "en": "Choose the Gogs administrator (must be an existing YunoHost user)",
                    "fr": "Choisissez l'administrateur de Gogs (doit Ãªtre un utilisateur YunoHost existant)"
                },
                "example": "johndoe"
            },
            {
                "name": "public_site",
                "ask": {
                    "en": "Is it a public site ?",
                    "fr": "Est-ce un site public ?"
                },
                "choices": ["Yes", "No"],
                "default": "Yes"
            }
        ]
    }
}
